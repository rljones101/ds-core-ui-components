{
  "name": "core-ui-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "cross-env vue-cli-service serve",
    "build": "cross-env npm run build-vue-components",
    "build-vue-components": "cross-env vue-cli-service build --target lib --dest 'dist/vueComponents' --name core-ui './src/index.js'",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint": "cross-env vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-code-highlight": "^0.7.8",
    "vue-ctk-date-time-picker": "^2.5.0",
    "vue-material": "*"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.10",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "ignorePatterns": [
      "README.md"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
